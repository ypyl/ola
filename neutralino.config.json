{
  "applicationId": "js.neutralino.zero",
  "version": "0.0.3",
  "defaultMode": "window",
  "documentRoot": "/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "clipboard.*",
    "events.*"
  ],
  "modes": {
    "window": {
      "title": "ola",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "enableInspector": true,
      "borderless": false,
      "icon": "/build/icon.png"
    }
  },
  "cli": {
    "binaryName": "ola",
    "resourcesPath": "/build/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "5.1.0",
    "clientVersion": "5.1.0",
    "frontendLibrary": {
      "patchFile": "index.html",
      "devUrl": "http://localhost:5678"
    }
  }
}
